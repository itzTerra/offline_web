{
    "name": "Github Page PWA",
    "short_name": "GPPWA",
    "description": "Github Page as a Progressive Web App",
   
    "scope": "/offline_web/",
    "start_url": "/offline_web/",
    "offline_page": "/index.html",
    "fallbacks": {
        "/": "/index.html"
    },

    "background_color": "#ffffff",
    "theme_color": "#ffffff",
   
    "display": "standalone", 
   
    "icons": [
        {
          "src": "/offline_web/favicon.jpg",
          "type": "image/png",
          "sizes": "700x700"
        }
    ]
  }